{
	"name": "squeeze",
	"displayName": "Squeeze",
	"description": "Compress terminal logs and ask Copilot",
	"version": "0.0.1",
	"publisher": "trae-user",
	"engines": {
		"vscode": "^1.90.0"
	},
	"categories": [
		"AI",
		"Chat"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"chatParticipants": [
			{
				"id": "squeeze",
				"name": "squeeze",
				"description": "Compress context files before sending to LLM",
				"isSticky": true
			}
		],
		"commands": [],
		"configuration": {
			"title": "Squeeze",
			"properties": {
				"squeeze.model": {
					"type": "string",
					"default": "default",
					"description": "Compression model to use (sent to API)"
				},
				"squeeze.apiEndpoint": {
					"type": "string",
					"default": "http://localhost:8000/compress",
					"description": "API endpoint URL for compression service"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.90.0",
		"@types/node": "20.x",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3",
		"@vscode/test-cli": "^0.0.4",
		"@vscode/test-electron": "^2.3.9"
	},
	"dependencies": {}
}
